{
    "compilerOptions": {
        "noImplicitAny": false,
        "allowSyntheticDefaultImports": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "strictNullChecks": true,
        "noEmit": false
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
}